{
  "settings": {
    "index": {
      "number_of_shards": 5,
      "max_result_window": 2147483647
    },
    "analysis": {
      "analyzer": {
        "paths": {
          "tokenizer": "path_hierarchy"
        },
        "path": {
          "type": "pattern",
          "pattern": "/"
        }
      }
    }
  },
  "mappings": {
    "_default_": {
      "_all": {
        "enabled": true
      },
      "dynamic_templates": [
        {
          "strings" : {
            "match_mapping_type": "string",
            "mapping": {
              "type": "text"
            }
          }
        }
      ],
      "properties": {
        "audit": {
          "properties": {
            "createdBy": {
              "type": "keyword",
              "index": true
            },
            "lastModifiedBy": {
              "type": "keyword",
              "index": true
            }
          }
        },
        "serde": {
          "properties": {
            "inputFormat": {
              "type": "text",
              "index": true,
              "fields": {
                "keyword": {
                  "type": "keyword",
                  "ignore_above": 256
                }
              }
            },
            "outputFormat": {
              "type": "text",
              "index": true,
              "fields": {
                "keyword": {
                  "type": "keyword",
                  "ignore_above": 256
                }
              }
            },
            "owner": {
              "type": "text",
              "index": true,
              "fields": {
                "keyword": {
                  "type": "keyword",
                  "ignore_above": 256
                }
              }
            },
            "serializationLib": {
              "type": "text",
              "index": true,
              "fields": {
                "keyword": {
                  "type": "keyword",
                  "ignore_above": 256
                }
              }
            },
            "uri": {
              "type": "keyword",
              "index": true,
              "fields": {
                "tree": {
                  "type": "text",
                  "analyzer": "paths"
                }
              }
            }
          }
        },
        "searchableDefinitionMetadata": {
          "properties": {
            "job": {
              "properties": {
                "name": {
                  "type": "keyword",
                  "index": true
                }
              }
            },
            "owner": {
              "properties": {
                "name": {
                  "type": "keyword",
                  "index": true
                },
                "team": {
                  "type": "keyword",
                  "index": true
                },
                "userId": {
                  "type": "keyword",
                  "index": true
                }
              }
            },
            "tags": {
              "type": "keyword",
              "index": true
            },
            "data_hygiene": {
              "properties": {
                "delete_column": {
                  "type": "keyword",
                  "index": true
                },
                "delete_method": {
                  "type": "keyword",
                  "index": true
                }
              }
            },
            "lifetime": {
              "properties": {
                "partitionedBy": {
                  "type": "keyword",
                  "index": true
                },
                "user": {
                  "type": "text",
                  "index": true,
                  "fields": {
                    "keyword": {
                      "type": "keyword",
                      "ignore_above": 256
                    }
                  }
                }
              }
            }
          }
        },
        "definitionMetadata": {
          "type": "object",
          "enabled": false
        },
        "dataMetadata": {
          "type": "object",
          "enabled": false
        },
        "name": {
          "properties": {
            "catalogName": {
              "type": "keyword",
              "index": true
            },
            "databaseName": {
              "type": "keyword",
              "index": true
            },
            "qualifiedName": {
              "type": "keyword",
              "index": true,
              "fields": {
                "tree": {
                  "type": "text",
                  "analyzer": "paths"
                }
              }
            },
            "tableName": {
              "type": "keyword",
              "index": true
            },
            "viewName": {
              "type": "keyword",
              "index": true
            },
            "partitionName": {
              "type": "keyword",
              "index": true,
              "fields": {
                "keys": {
                  "type": "text",
                  "analyzer": "path"
                }
              }
            }
          }
        },
        "timestamp": {
          "type": "date",
          "format": "epoch_millis",
          "index": true
        },
        "user_": {
          "type": "keyword",
          "index": true
        },
        "type": {
          "type": "keyword",
          "index": true
        },
        "refreshMarker_": {
          "type": "keyword",
          "index": true
        }
      }
    },
    "table": {
      "properties": {
        "fields": {
          "properties": {
            "name": {
              "type": "keyword",
              "index": true
            },
            "type": {
              "type": "keyword",
              "index": true
            },
            "source_type": {
              "type": "keyword",
              "index": true
            }
          }
        }
      }
    },
    "mview": {
      "properties": {
        "fields": {
          "properties": {
            "name": {
              "type": "keyword",
              "index": true
            },
            "type": {
              "type": "keyword",
              "index": true
            }
          }
        }
      }
    },
    "metacat-log": {
      "properties": {
        "method": {
          "type": "keyword",
          "index": true
        },
        "type": {
          "type": "keyword",
          "index": true
        },
        "data": {
          "type": "keyword",
          "index": true
        },
        "message": {
          "type": "keyword",
          "index": true
        },
        "details": {
          "type": "keyword",
          "index": true
        }
      }
    }
  }
}
